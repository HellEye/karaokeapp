	"scripts": {
		"react:start": "react-scripts start",
		"build-task:scss-compile": "node-sass-chokidar --source-map true -rw src/res/scss/ -o src/res/css",
		"sass:build": "npm-run-all -p build-task:*",
		"sass:watch": "chokidar 'src/scss/**/*.s{c,a}ss' -c 'npm run sass:build'",
		"dev:sass": "npm-run-all -p sass:*",
		"sass:run": "npm run build-task:scss-compile",
		"electron:start": "wait-on tcp:5678 && electron .",
		"electron:build": "",
		"dev": "concurrently -t \"hh:mm:ss\" --prefix \"{time} {name}\" --names \"NPM ,ELEC\" --restart-tries=-1 --restart-after=1000 \"cross-env BROWSER=none PORT=5678 npm run react:start\" \"cross-env NODE_ENV=development npm run electron:start\""
	},
	"main": "public/main.js",
	"homepage": "./",
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
